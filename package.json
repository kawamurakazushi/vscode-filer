{
  "name": "vscode-filer",
  "displayName": "vscode-filer",
  "description": "A File Explorer",
  "version": "0.0.2",
  "publisher": "kawamurakazushi",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:filer.show"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "filer.show",
        "title": "Show Filer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kawamurakazushi/vscode-filer"
  }
}
